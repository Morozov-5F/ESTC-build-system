    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/main.elf",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "text": "tar ext :4242"
                },
                {
                    "text": "load ${fileDirname}/main.elf"
                }
            ],
            "debugServerPath": "/usr/bin/st-util",
        }
    ]
}